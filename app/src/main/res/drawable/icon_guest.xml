<?xml version="1.0" encoding="UTF-8"?>
<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="guestGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3A86FF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2B6CB0;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="64" cy="64" r="50" 
          fill="url(#guestGradient)" 
          stroke="none"/>
  
  <!-- User silhouette -->
  <g transform="translate(64, 64)">
    <!-- Head -->
    <circle cx="0" cy="-15" r="12" 
            fill="#FFFFFF" 
            stroke="none"/>
    
    <!-- Body -->
    <path d="M-15 0 C-15 -5, -10 -10, 0 -10 C10 -10, 15 -5, 15 0 L15 25 C15 30, 10 35, 0 35 C-10 35, -15 30, -15 25 Z" 
          fill="#FFFFFF" 
          stroke="none"/>
    
    <!-- Arms -->
    <ellipse cx="-20" cy="5" rx="6" ry="15" 
             fill="#FFFFFF" 
             transform="rotate(-20 -20 5)"/>
    <ellipse cx="20" cy="5" rx="6" ry="15" 
             fill="#FFFFFF" 
             transform="rotate(20 20 5)"/>
    
    <!-- Legs -->
    <ellipse cx="-8" cy="40" rx="6" ry="12" 
             fill="#FFFFFF"/>
    <ellipse cx="8" cy="40" rx="6" ry="12" 
             fill="#FFFFFF"/>
  </g>
  
  <!-- Guest indicator badge -->
  <circle cx="90" cy="30" r="12" 
          fill="#06D6A0" 
          stroke="#FFFFFF" 
          stroke-width="2"/>
  
  <!-- Guest icon in badge -->
  <text x="90" y="36" 
        font-family="Inter, sans-serif" 
        font-size="12" 
        font-weight="700" 
        text-anchor="middle" 
        fill="#FFFFFF">
    G
  </text>
  
  <!-- Subtle pattern overlay -->
  <g opacity="0.1">
    <circle cx="20" cy="20" r="2" fill="#FFFFFF"/>
    <circle cx="108" cy="20" r="2" fill="#FFFFFF"/>
    <circle cx="20" cy="108" r="2" fill="#FFFFFF"/>
    <circle cx="108" cy="108" r="2" fill="#FFFFFF"/>
  </g>
</svg>
